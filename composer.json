{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "balping/laravel-hashslug": "^2.2",
        "barryvdh/laravel-dompdf": "^2.2",
        "cviebrock/eloquent-sluggable": "^10.0",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^3.8",
        "endroid/qr-code": "^5.0",
        "ezyang/htmlpurifier": "^4.18",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/pagination": "*",
        "intervention/image": "2.7",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "mews/purifier": "^3.4",
        "milon/barcode": "^11.0",
        "munafio/chatify": "^1.6",
        "nesbot/carbon": "^2.73",
        "php-mcp/laravel": "3.0",
        "pusher/pusher-php-server": "^7.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/crawler": "^8.2",
        "square/square": "39.0.0.20241120",
        "symfony/http-foundation": "^6.4",
        "symfony/process": "^6.4",
        "voku/portable-ascii": "^2.0",
        "voku/portable-utf8": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Http\\Controllers\\Frontend\\": "app/Http/Controllers/Frontend/",
            "App\\Models\\": "app/Models/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\Http\\Controllers\\Website\\": "app/Http/Controllers/Website/"
        },
        "files": [
            "config/constants.php",
            "app/Helpers/helpers.php",
            "App/Helpers/FooterDataHelper.php",
            "app/Helpers/ImageHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "audit": {
            "abandoned": "report"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
